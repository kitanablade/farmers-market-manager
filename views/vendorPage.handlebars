<aside>
    <div class="container text-center">
    <div class="row">
        <div class="col-12 sidebar">
        <a href="/" id="home">Home</a>
        </div>
        <div class="col-12 sidebar">
        <a href="/event" id="events">Event List</a> 
        </div>
        <div class="col-12 sidebar">
        <a href="/vendor" id="vendors"> Vendor List</a>
        </div>
    </div>
</aside>

<section id="eventsProducts">
    
        <h3>{{vendorName}}</h3>
        <img src={{logo_url}} alt="" style="object-fit:cover;
            width:500px;
            height:500px;"/>
        {{#each Products}}
        <div class= "card">
        <h4>{{productName}}</h4>
        <img src={{img_url}} alt="" style="object-fit:cover;
            width:100px;
            height:100px;"/>
        <ul>
            <li>Description: {{description}}</li>
            <li>In Stock: {{inStock}}</li>
        </ul>
        </div>
        {{/each}}
    
</section>