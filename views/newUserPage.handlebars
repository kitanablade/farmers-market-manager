
<section>
        <div class="jumbotron">
            <div class="container" id="loginJumbo">
            <h1 id="imgJumbo"> Welcome, our newest Vendors!</h1>

            </div>
        </div>
        </section>
        <section>
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <form role="form" enctype="application/x-www-form-urlencoded" id="signup-form">
                        <legend>Enter Required Registration Information!</legend>

                        <div class="form-group">
                            <label for="first_name">Vendor Name:</label>
                            <input type="text" class="form-control" name="first_name" id="signup-username"
                                placeholder="first name">
                        </div>

                        <div class="form-group">
                            <label for="description">About Yourself:</label>
                            <input type="text" class="form-control" name="description" id="signup-description"
                            placeholder="I sell eggs in beautiful Snohomish, WA">
                    </div>

                    <div class="form-group">
                        <label for="email">Email Address:</label>
                            <input type="text" class="form-control" name="email" id="signup-email" placeholder="email address">
                    </div>

                    <div class="form-group">
                        <label for="pw">Password:</label>
                        <input type="password" class="form-control" name="pw" id="signup-password">
                    </div>

                        <button id="upload_widget" class="cloudinary-button">Upload Logo</button>

                        {{!-- <div class="form-group">
                            <label for="pw_confirm">Confirm Password</label>
                            <input type="password" class="form-control" name="pw_confirm" id="">
                        </div> --}}

                        <button id="submit-button" type="submit" class="btn btn-primary">Sign up</button>

                </form>
            </div>
        </div>
        </div>
        </section>
        
        <script src="https://upload-widget.cloudinary.com/global/all.js" type="text/javascript"></script>
